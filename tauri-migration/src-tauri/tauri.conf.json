{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RegenBrowser",
  "version": "0.1.0",
  "identifier": "in.omnibrowser.regen",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "RegenBrowser",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "url": "/"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
