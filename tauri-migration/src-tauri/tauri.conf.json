{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RegenBrowser",
  "version": "0.1.0",
  "identifier": "in.omnibrowser.regen",
  "build": {
    "beforeDevCommand": "node scripts/noop.js",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "RegenBrowser",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "url": "/",
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": "default-src * 'self' 'unsafe-inline' 'unsafe-eval' data: blob: https: wss: ws: http://localhost:*; img-src * data: blob: https:; media-src * data: blob: https:; font-src * data: https:; connect-src * wss: ws: https: http://localhost:*; script-src * 'unsafe-inline' 'unsafe-eval' https:; style-src * 'unsafe-inline' https:;"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
