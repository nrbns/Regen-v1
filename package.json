{
  "name": "regen",
  "version": "0.1.0-alpha",
  "private": true,
  "description": "Regen/Regen: Next-generation AI automation platform with full agent layer, automation workflows, Redix execution environment, and multi-browser capabilities",
  "scripts": {
    "setup:ollama": "echo 'Ollama setup: Install from https://ollama.com/download. Then run: ollama pull phi3:mini && ollama pull llava:7b'",
    "test:all": "npm run test:unit && npm run lint && npm run build && npm run audit:prod",
    "test:openai": "cd apps/api && python test_openai.py",
    "dev:api": "python -m uvicorn apps.api.main:app --reload --port 8000",
    "dev:engine": "cd packages/omni-engine && npm run dev",
    "dev": "concurrently \"npm:dev:web\" \"npm:dev:server\" \"npm:dev:tauri\" \"npm:worker:llm\"",
    "dev:all": "node scripts/start-dev-all.cjs",
    "dev:web": "npx cross-env JSDOM_NO_CANVAS=1 vite --mode development",
    "dev:full": "npm run dev",
    "dev:server": "node server/redix-server.js",
    "dev:realtime-server": "node server/realtime-server.js",
    "dev:tauri": "cd tauri-migration && npm run tauri dev",
    "dev:realtime": "concurrently \"npm:dev:server\" \"npm:dev:tauri\" \"npm:worker:llm\"",
    "dev:trade": "concurrently \"npm:dev:realtime-server\" \"npm:dev:web\"",
    "worker:llm": "node server/services/queue/llmWorker.js",
    "worker:research": "node server/services/queue/researchWorker.js",
    "test:research-flow": "node tools/test-research-flow.js",
    "worker:scraper": "node server/services/queue/worker.js",
    "build": "npx cross-env JSDOM_NO_CANVAS=1 vite build",
    "build:types": "npx tsc -b",
    "build:app": "cd tauri-migration && npm run tauri build",
    "build:windows": "npm run build && cd tauri-migration && npm run tauri:build -- --target x86_64-pc-windows-msvc",
    "build:windows:installer": "npm run build && cd tauri-migration && npm run tauri:build -- --bundles msi",
    "build:installer": "node scripts/build-installer-with-ollama.js",
    "setup:playwright": "node scripts/setup-playwright.js",
    "build:android": "npm run build && npm run build:android:apk",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:android:setup": "npm run build && npx cap add android && npx cap sync android",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest run",
    "test:unit": "vitest run",
    "pretest:unit": "node scripts/ensure-single-react.js",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "storybook": "npx storybook dev -p 6006",
    "storybook:build": "npx storybook build",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:runtime": "node scripts/runtime-test.js",
    "test:load": "k6 run load-test/k6-summarize.js",
    "test:load:sse": "k6 run load-test/k6-sse.js",
    "test:multilingual-offline": "playwright test tests/e2e/multilingual-offline.spec.ts --config=playwright.config.ts",
    "test:backend": "playwright test tests/e2e/backend-api.spec.ts --config=playwright.config.ts",
    "test:research": "playwright test tests/e2e/research-streaming.spec.ts --config=playwright.config.ts",
    "test:visual": "playwright test --config=playwright.visual.config.ts",
    "test:visual:update": "playwright test --config=playwright.visual.config.ts --update-snapshots",
    "dev:redix": "tsx server/redix-core.ts",
    "dev:redix-universal": "vite --config vite.config.redix.ts",
    "build:redix": "vite build --config vite.config.redix.ts",
    "audit:lighthouse": "tsx scripts/lighthouse-audit.ts",
    "perf:test": "tsx scripts/perf/perf-audit.ts",
    "perf:ci": "tsx scripts/perf/ci-perf-benchmark.ts",
    "test:bounds": "playwright test tests/e2e/webview-bounds.spec.ts --config=playwright.config.ts",
    "postinstall": "node ./scripts/postinstall.ts",
    "build:lunr": "node scripts/build-lunr.js",
    "dev:fast": "npx cross-env JSDOM_NO_CANVAS=1 OB_DISABLE_HEAVY_SERVICES=1 vite --mode development",
    "clean": "node scripts/clean-cache.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "clean:all": "npm run clean && rimraf node_modules && npm install",
    "kill:dev": "node scripts/kill-dev-processes.js",
    "audit:prod": "npm audit --omit=dev",
    "ci:check": "npm run build:types && npm run lint && npm run audit:prod && npm run perf:test",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"*.{js,json,md}\"",
    "verify:build": "node scripts/verify-build.js",
    "changelog": "node scripts/generate-changelog.js",
    "scraper:worker": "node server/services/queue/worker.js",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/websocket": "^11.2.0",
    "@langchain/anthropic": "^1.0.1",
    "@langchain/core": "^1.0.5",
    "@langchain/openai": "^1.1.1",
    "@mozilla/readability": "^0.6.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@sentry/react": "^10.27.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/dompurify": "^3.0.5",
    "abort-controller": "^3.0.0",
    "bufferutil": "file:stubs/bufferutil-stub",
    "bullmq": "^5.64.0",
    "canvas": "file:stubs/canvas-stub",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.8",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4.5.0",
    "fastify": "^5.6.2",
    "framer-motion": "^11.18.2",
    "franc": "^6.1.0",
    "hnswlib-wasm": "^0.8.2",
    "html-entities": "^2.5.2",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.8.2",
    "langchain": "^1.0.4",
    "lightweight-charts": "^5.0.9",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.453.0",
    "lunr": "^2.3.9",
    "mammoth": "^1.6.0",
    "meilisearch": "^0.40.0",
    "murmurhash3js": "^3.0.1",
    "node-fetch": "^3.3.2",
    "opossum": "^9.0.0",
    "p-queue": "^8.0.1",
    "p-timeout": "^7.0.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.8.69",
    "pino": "^10.1.0",
    "playwright": "^1.56.1",
    "playwright-core": "^1.56.1",
    "radix-ui": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^6.26.1",
    "reactflow": "^11.10.3",
    "robots-parser": "^3.0.1",
    "shadcn-ui": "^0.9.4",
    "tailwindcss": "^3.4.14",
    "tough-cookie": "^4.1.4",
    "ts-pattern": "^5.1.2",
    "utf-8-validate": "file:stubs/utf-8-validate-stub",
    "uuid": "^13.0.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27",
    "zod": "^3.25.76",
    "zustand": "^4.5.2"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.6.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/js": "^9.10.0",
    "@playwright/test": "^1.56.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@storybook/addon-a11y": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.11.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.12",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.1",
    "fake-indexeddb": "^6.2.5",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jscodeshift": "^0.15.1",
    "jsdom": "^27.2.0",
    "puppeteer": "^23.11.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "semantic-release": "^25.0.2",
    "storybook": "^8.6.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.14",
    "wait-on": "^7.2.0",
    "ws": "^8.18.3"
  },
  "build": {
    "appId": "com.example.regen",
    "productName": "Regen",
    "files": [
      "dist-web/**",
      "package.json"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist-release"
    },
    "publish": {
      "provider": "github",
      "owner": "nrbns",
      "repo": "Omnibrowser"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "artifactName": "${productName}-${version}-mac.${ext}",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-win.${ext}",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "certificateFile": "${env.WIN_CERT_FILE}",
      "certificatePassword": "${env.WIN_CERT_PASSWORD}",
      "timeStampServer": "http://timestamp.digicert.com"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility",
      "artifactName": "${productName}-${version}-linux.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
