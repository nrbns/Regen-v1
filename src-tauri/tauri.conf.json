{
  "productName": "Omnibrowser",
  "version": "0.1.0",
  "identifier": "com.omnibrowser.app",
  "build": {
    "beforeDevCommand": "npm run dev:web",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist-web"
  },
  "app": {
    "windows": [
      {
        "title": "Omnibrowser",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
