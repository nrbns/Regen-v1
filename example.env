# OmniBrowser Example Environment Variables
# Copy to `.env` and fill in real values as needed

# --- Build / Runtime ---
NODE_ENV=development
VITE_DEV_SERVER_URL=http://localhost:5173

# --- API Security ---
API_BASE_URL=http://localhost:8000
JWT_SECRET=replace-with-strong-secret
ACCESS_TOKEN_EXPIRE_MINUTES=60
REFRESH_TOKEN_EXPIRE_DAYS=30

# --- Cache / Queues ---
REDIS_URL=redis://localhost:6379/0

# Electron dev safety toggles (keep unless you know you can disable)
OB_DISABLE_HEAVY_SERVICES=1
ELECTRON_DISABLE_GPU_SANDBOX=1
LIBANGLE_DISABLE_D3D11=1
ANGLE_DEFAULT_PLATFORM=swiftshader
ELECTRON_FORCE_USE_SWIFTSHADER=1
ELECTRON_ENABLE_STACK_DUMPING=true
ELECTRON_DEFAULT_ERROR_MODE=true

# --- Trading APIs ---
BINANCE_API_KEY=
BINANCE_API_SECRET=
FINNHUB_API_KEY=

# --- Research / Search APIs ---
BRAVE_API_KEY=
BING_API_KEY=your_bing_api_key_here
TAVILY_API_KEY=
FIRECRAWL_API_KEY=
FIRECRAWL_BASE_URL=https://api.firecrawl.dev
FIRECRAWL_WEBHOOK_SECRET=
ZYTE_API_KEY=

# Search Proxy Server (runs on port 3001 by default)
SEARCH_PROXY_PORT=3001
SEARCH_PROXY_HOST=0.0.0.0
VITE_SEARCH_PROXY_URL=http://localhost:3001

# --- Gaming Integrations ---
CRAZY_GAMES_TOKEN=
POKI_API_KEY=
GAMEMONETIZE_API_KEY=
ITCHIO_CLIENT_ID=
RAWG_API_KEY=

# --- Identity / Redix ---
REDIX_IDENTITY_API_URL=https://localhost:8443
REDIX_IDENTITY_API_KEY=

# --- OpenAI / ChatGPT (primary AI backend) ---
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Bing Search API (optional, for enhanced search aggregation)
BING_API_KEY=your_bing_api_key_here

# --- Redix Core (Green Intelligence Engine) ---
REDIX_PORT=8001
VITE_REDIX_CORE_URL=http://localhost:8001

# --- Hugging Face AI (for local embeddings and chat) ---
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_BASE_URL=https://router.huggingface.co/hf-inference

# --- Anthropic (Claude) ---
ANTHROPIC_API_KEY=
ANTHROPIC_BASE_URL=https://api.anthropic.com/v1

# --- Ollama (local LLM fallback) ---
OLLAMA_BASE_URL=http://localhost:11434

# --- Knowledge Graph / Cloud Services ---
KNOWLEDGE_GRAPH_API_URL=
KNOWLEDGE_GRAPH_API_KEY=

# --- Eco Impact / Carbon ---
CARBON_API_TOKEN=
CARBON_REGION_CODE=US-NY

# --- AI Response Cache (optional) ---
AI_CACHE_ENABLED=true
AI_CACHE_MAX_ENTRIES=1000
AI_CACHE_TTL_SEARCH=1800
AI_CACHE_TTL_CHAT=7200
AI_CACHE_TTL_DEFAULT=3600

# --- Research Scraper ---
SCRAPER_MAX_CONCURRENCY=4
SCRAPER_TIMEOUT_SECONDS=12
SCRAPER_CACHE_TTL=1800
SCRAPER_RENDER_ENDPOINT=
SCRAPER_USE_RENDER_FALLBACK=true

# --- Telemetry (optional) ---
OTEL_ENABLED=false
OTEL_EXPORTER_OTLP_ENDPOINT=
TELEMETRY_ENDPOINT=https://telemetry.omnibrowser.dev/api/v1/events
TELEMETRY_ENABLED=false
SENTRY_DSN=
SENTRY_TRACES_SAMPLE_RATE=0.0
VITE_SENTRY_DSN=
VITE_SENTRY_TRACES_SAMPLE_RATE=0.0
ANALYTICS_ENDPOINT=https://analytics.example.com/api/events
ANALYTICS_SITE_ID=omnibrowser-app
ANALYTICS_PUBLIC_KEY=
